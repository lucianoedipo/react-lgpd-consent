{
  "name": "react-lgpd-consent",
  "version": "0.4.1",
  "description": "Biblioteca de consentimento LGPD, integrações nativas e sistema extensível para React.",
  "keywords": [
    "lgpd",
    "anpd",
    "cookie",
    "consent",
    "react",
    "client-side",
    "spa",
    "material-ui",
    "mui",
    "typescript",
    "js-cookie",
    "consentimento",
    "privacidade",
    "acessibilidade",
    "google-analytics",
    "tag-manager",
    "userway",
    "categorias-extensiveis"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Luciano Édipo",
    "email": "luciano.psilva@anpd.gov.br",
    "url": "https://github.com/lucianoedipo"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "README.en.md",
    "QUICKSTART.md",
    "QUICKSTART.en.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "npm@11.6.0",
  "scripts": {
    "clean": "rimraf node_modules package-lock.json dist docs storybook-static .cache .stryker-tmp",
    "build": "cross-env NODE_ENV=production tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "cross-env NODE_ENV=development tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint \"**/*.{ts,tsx}\" --cache",
    "lint:ci": "eslint \"**/*.{ts,tsx}\" --cache --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\" \"eslint.config.js\"",
    "type-check": "tsc --noEmit",
    "test": "jest --config jest.config.ts",
    "test:coverage": "jest --config jest.config.ts --coverage",
    "coverage-check": "node scripts/coverage-check.cjs",
    "size-check": "size-limit",
    "mutation": "npx stryker run",
    "mutation:tests": "npx stryker run ./stryker.tests.conf.json",
    "prepublishOnly": "npm run build",
    "docs:generate": "typedoc --out docs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucianoedipo/react-lgpd-consent.git"
  },
  "bugs": {
    "url": "https://github.com/lucianoedipo/react-lgpd-consent/issues"
  },
  "homepage": "https://lucianoedipo.github.io/react-lgpd-consent/",
  "peerDependencies": {
    "@mui/icons-material": "^7.0.0 || ^6.0.0 || ^5.15.0",
    "@mui/material": "^7.0.0 || ^6.0.0 || ^5.15.0",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@mui/icons-material": {
      "optional": true
    }
  },
  "dependencies": {
    "js-cookie": "^3.0.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.1",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@eslint/compat": "1.3.2",
    "@mui/icons-material": "7.3.2",
    "@mui/material": "7.3.2",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@storybook/addon-a11y": "9.1.7",
    "@storybook/addon-docs": "9.1.7",
    "@storybook/addon-vitest": "9.1.7",
    "@storybook/react-vite": "9.1.7",
    "@stryker-mutator/core": "9.1.1",
    "@stryker-mutator/jest-runner": "9.1.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/js-cookie": "3.0.6",
    "@types/node": "24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "cross-env": "10.0.0",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "9.1.7",
    "globals": "^16.4.0",
    "jest": "30.1.3",
    "jest-environment-jsdom": "30.1.2",
    "prettier": "3.6.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "rimraf": "6.0.1",
    "size-limit": "^11.2.0",
    "storybook": "9.1.10",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "tsup": "8.5.0",
    "typedoc": "0.28.13",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0"
  },
  "size-limit": [
    {
      "name": "ESM Bundle",
      "path": "dist/index.js",
      "limit": "17 KB"
    },
    {
      "name": "CJS Bundle",
      "path": "dist/index.cjs",
      "limit": "75 KB"
    },
    {
      "name": "Types",
      "path": "dist/index.d.ts",
      "limit": "100 KB"
    },
    {
      "name": "Complete Package Import",
      "path": "dist/index.js",
      "import": "{ ConsentProvider, useConsent, ConsentScriptLoader }",
      "limit": "15 KB"
    }
  ]
}
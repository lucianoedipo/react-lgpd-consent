{
  "name": "@react-lgpd-consent/workspace",
  "private": true,
  "version": "0.5.4",
  "type": "module",
  "description": "Workspace PNPM para os pacotes react-lgpd-consent.",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.29.1",
  "pnpm": {
    "overrides": {
      "react": "19.2.4",
      "react-dom": "19.2.4",
      "qs": ">=6.14.1",
      "diff": ">=4.0.4"
    },
    "patchedDependencies": {
      "@mui/icons-material@7.3.7": "patches/@mui__icons-material@7.3.7.patch"
    }
  },
  "scripts": {
    "clean": "turbo run clean",
    "build": "turbo run build",
    "build:core": "turbo run build --filter=@react-lgpd-consent/core",
    "build:mui": "turbo run build --filter=@react-lgpd-consent/mui",
    "build:main": "turbo run build --filter=react-lgpd-consent",
    "dev": "turbo run dev --filter=react-lgpd-consent",
    "lint": "turbo run lint",
    "lint:ci": "turbo run lint:ci",
    "format": "pnpm -r run format",
    "type-check": "turbo run type-check",
    "test": "NODE_OPTIONS='--max-old-space-size=4096' turbo run test",
    "test:a11y": "turbo run test:a11y --filter=react-lgpd-consent",
    "test:coverage": "turbo run test:coverage --filter=react-lgpd-consent",
    "coverage-check": "pnpm --filter react-lgpd-consent coverage-check",
    "coverage:analyze": "bash scripts/coverage-analysis.sh",
    "size-check": "pnpm --filter react-lgpd-consent size-check",
    "mutation": "npx stryker run stryker.conf.json",
    "mutation:tests": "npx stryker run stryker.tests.conf.json",
    "docs:generate": "turbo run docs:generate",
    "storybook": "turbo run storybook",
    "build-storybook": "turbo run build-storybook",
    "changeset": "changeset",
    "changeset:interactive": "pnpm exec tsx scripts/changeset-interactive.ts",
    "changeset:version": "changeset version && node scripts/sync-versions.mjs && pnpm install --lockfile-only",
    "changeset:publish": "changeset publish",
    "release": "turbo run build lint test && changeset publish"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.1",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@chromatic-com/storybook": "5.0.1",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@eslint/compat": "2.0.2",
    "@eslint/js": "^9.39.2",
    "@mui/icons-material": "7.3.7",
    "@mui/material": "7.3.7",
    "@size-limit/preset-small-lib": "^12.0.0",
    "@storybook/addon-a11y": "10.2.7",
    "@storybook/addon-docs": "10.2.7",
    "@storybook/addon-vitest": "10.2.7",
    "@storybook/react-vite": "10.2.7",
    "@stryker-mutator/core": "9.5.1",
    "@stryker-mutator/jest-runner": "9.5.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/js-cookie": "3.0.6",
    "@types/node": "25.2.2",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "baseline-browser-mapping": "^2.9.19",
    "cross-env": "10.1.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.13.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-storybook": "10.2.7",
    "globals": "^17.3.0",
    "jest": "30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "30.2.0",
    "prettier": "3.8.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "rimraf": "6.1.2",
    "size-limit": "^12.0.0",
    "storybook": "10.2.7",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tsup": "8.5.1",
    "tsx": "^4.21.0",
    "turbo": "^2.8.3",
    "typedoc": "0.28.16",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0"
  }
}

{
  "name": "@react-lgpd-consent/workspace",
  "private": true,
  "version": "0.5.4",
  "type": "module",
  "description": "Workspace PNPM para os pacotes react-lgpd-consent.",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402",
  "pnpm": {
    "overrides": {
      "react": "19.2.3",
      "react-dom": "19.2.3"
    }
  },
  "scripts": {
    "clean": "turbo run clean",
    "build": "turbo run build",
    "build:core": "turbo run build --filter=@react-lgpd-consent/core",
    "build:mui": "turbo run build --filter=@react-lgpd-consent/mui",
    "build:main": "turbo run build --filter=react-lgpd-consent",
    "dev": "turbo run dev --filter=react-lgpd-consent",
    "lint": "turbo run lint",
    "lint:ci": "turbo run lint:ci",
    "format": "pnpm -r run format",
    "type-check": "turbo run type-check",
    "test": "NODE_OPTIONS='--max-old-space-size=4096' turbo run test",
    "test:a11y": "turbo run test:a11y --filter=react-lgpd-consent",
    "test:coverage": "turbo run test:coverage --filter=react-lgpd-consent",
    "coverage-check": "pnpm --filter react-lgpd-consent coverage-check",
    "size-check": "pnpm --filter react-lgpd-consent size-check",
    "mutation": "npx stryker run stryker.conf.json",
    "mutation:tests": "npx stryker run stryker.tests.conf.json",
    "docs:generate": "turbo run docs:generate",
    "storybook": "turbo run storybook",
    "build-storybook": "turbo run build-storybook",
    "changeset": "changeset",
    "changeset:version": "changeset version && node scripts/sync-versions.mjs && pnpm install --lockfile-only",
    "changeset:publish": "changeset publish",
    "release": "turbo run build lint test && changeset publish"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@chromatic-com/storybook": "4.1.3",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@eslint/compat": "2.0.0",
    "@eslint/js": "^9.39.2",
    "@mui/icons-material": "7.3.6",
    "@mui/material": "7.3.6",
    "@size-limit/preset-small-lib": "^12.0.0",
    "@storybook/addon-a11y": "10.1.9",
    "@storybook/addon-docs": "10.1.9",
    "@storybook/addon-vitest": "10.1.9",
    "@storybook/react-vite": "10.1.9",
    "@stryker-mutator/core": "9.4.0",
    "@stryker-mutator/jest-runner": "9.4.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/js-cookie": "3.0.6",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.50.0",
    "@typescript-eslint/parser": "8.50.0",
    "cross-env": "10.1.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.11.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-storybook": "10.1.9",
    "globals": "^16.5.0",
    "jest": "30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "30.2.0",
    "prettier": "3.7.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rimraf": "6.1.2",
    "size-limit": "^12.0.0",
    "storybook": "10.1.10",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tsup": "8.5.1",
    "turbo": "^2.7.2",
    "typedoc": "0.28.15",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.0"
  }
}

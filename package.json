{
  "name": "@react-lgpd-consent/workspace",
  "private": true,
  "version": "0.5.4",
  "type": "module",
  "description": "Workspace PNPM para os pacotes react-lgpd-consent.",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "clean": "pnpm -r run clean",
    "build": "pnpm -r run build",
    "build:core": "pnpm --filter @react-lgpd-consent/core build",
    "build:mui": "pnpm --filter @react-lgpd-consent/mui build",
    "build:main": "pnpm --filter react-lgpd-consent build",
    "dev": "pnpm --filter react-lgpd-consent dev",
    "lint": "pnpm -r run lint",
    "lint:ci": "pnpm -r run lint:ci",
    "format": "pnpm -r run format",
    "type-check": "pnpm -r run type-check",
    "test": "pnpm -r run test",
    "test:a11y": "pnpm --filter react-lgpd-consent test:a11y",
    "test:coverage": "pnpm --filter react-lgpd-consent test:coverage",
    "coverage-check": "pnpm --filter react-lgpd-consent coverage-check",
    "size-check": "pnpm --filter react-lgpd-consent size-check",
    "mutation": "pnpm --filter react-lgpd-consent mutation",
    "mutation:tests": "pnpm --filter react-lgpd-consent mutation:tests",
    "prepublishOnly": "pnpm --filter react-lgpd-consent prepublishOnly",
    "publish:core": "pnpm --filter @react-lgpd-consent/core publish",
    "publish:mui": "pnpm --filter @react-lgpd-consent/mui publish",
    "publish:main": "pnpm --filter react-lgpd-consent publish",
    "publish:all": "pnpm -r publish",
    "docs:generate": "pnpm --filter react-lgpd-consent docs:generate",
    "storybook": "pnpm --filter react-lgpd-consent storybook",
    "build-storybook": "pnpm --filter react-lgpd-consent build-storybook"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@chromatic-com/storybook": "4.1.2",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@eslint/compat": "1.4.1",
    "@eslint/js": "^9.39.1",
    "@mui/icons-material": "7.3.5",
    "@mui/material": "7.3.5",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@storybook/addon-a11y": "10.0.6",
    "@storybook/addon-docs": "10.0.6",
    "@storybook/addon-vitest": "10.0.6",
    "@storybook/react-vite": "10.0.6",
    "@stryker-mutator/core": "9.3.0",
    "@stryker-mutator/jest-runner": "9.3.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/js-cookie": "3.0.6",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "cross-env": "10.1.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-storybook": "10.0.6",
    "globals": "^16.5.0",
    "jest": "30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "30.2.0",
    "prettier": "3.6.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rimraf": "6.1.0",
    "size-limit": "^11.2.0",
    "storybook": "10.0.6",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "tsup": "8.5.0",
    "typedoc": "0.28.14",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.3"
  }
}

{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "mutate": [
    "packages/core/src/**/*.test.ts",
    "packages/core/src/**/*.test.tsx",
    "packages/mui/src/**/*.test.ts",
    "packages/mui/src/**/*.test.tsx",
    "packages/react-lgpd-consent/src/**/*.test.ts",
    "packages/react-lgpd-consent/src/**/*.test.tsx"
  ],
  "files": [
    "packages/core/src/**/*.ts",
    "packages/core/src/**/*.tsx",
    "packages/mui/src/**/*.ts",
    "packages/mui/src/**/*.tsx",
    "packages/react-lgpd-consent/src/**/*.ts",
    "packages/react-lgpd-consent/src/**/*.tsx",
    "jest.config.mjs"
  ],
  "testRunner": "command",
  "commandRunner": {
    "command": "pnpm test"
  },
  "buildCommand": "pnpm run type-check",
  "tsconfigFile": "tsconfig.base.json",
  "reporters": ["progress", "clear-text", "html"],
  "coverageAnalysis": "perTest",
  "concurrency": 4,
  "ignorePatterns": [
    "node_modules/**",
    "packages/**/dist/**",
    "example/**",
    "examples/**",
    "storybook-static/**",
    "docs/**",
    "scripts/**",
    "rfcs/**"
  ],
  "tempDirName": ".stryker-tmp",
  "cleanTempDir": true,
  "timeoutMS": 30000,
  "timeoutFactor": 2
}
